        <Label Content="IP-Nummer" Height="28" HorizontalAlignment="Left" Margin="37,102,0,0" Name="label1" VerticalAlignment="Top" Width="86" />
        <Label Content="IP-Name" Height="31" HorizontalAlignment="Left" Margin="37,37,0,0" Name="label2" VerticalAlignment="Top" Width="86" />
        <TextBox Height="23" Margin="169,104,55,0" Name="tbNumber" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="169,41,55,0" Name="tbName" VerticalAlignment="Top" />
        <Button Content="Name -&gt; Nummer" Height="42" HorizontalAlignment="Left" Margin="37,179,0,0" Name="cbToNumber" VerticalAlignment="Top" Width="172" Click="cbToNumber_Click" />
        <Button Content="Nummer -&gt; Name" Height="43" Margin="0,178,55,0" Name="cbToName" VerticalAlignment="Top" HorizontalAlignment="Right" Width="172" Click="cbToName_Click" />
    </Grid>
